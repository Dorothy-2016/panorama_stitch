    <IncludePath>D:/libs/opencv-3.2/build/include;$(IncludePath)</IncludePath>  
    <LibraryPath Condition="'$(Platform)'=='Win32'">D:/libs/opencv-3.2/build/x86/vc14/lib;$(LibraryPath)</LibraryPath>  
    <LibraryPath Condition="'$(Platform)'=='X64'">D:/libs/opencv-3.2/build/x64/vc14/lib;$(LibraryPath)</LibraryPath>  
