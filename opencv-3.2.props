    <Link Condition="'$(Configuration)'=='Release'">  
      <AdditionalDependencies>opencv_world320.lib;%(AdditionalDependencies)</AdditionalDependencies>  
    </Link>  
  </ItemDefinitionGroup>  
  <ItemGroup />  
